\line
if \u8220"tests\line
  then\line
    for FILE In S*\line
    do\line
      if test -h SFILE\line
      then\line
        echo \u8220"SFILE e' um link simb\u243 ?lico.\u8221"\line
      elif test -b SFILE\line
      then\line
        echo \u8220"SFILE e' um block device.\u8221"\line
      elif test -c $FILE\line
      then\line
        echo \u8220"SFILE e' um character device.\u8221"\line
      elif test -d $FILE\line
      then\line
        echo \u8220"SFILE e' um diretorio.\u8221"\line
      elif test -p $FILE\line
      then \line
        echo \u8220"SFILE e' um FIFO.\u8221"\line
      elif test -f $FILE\line
      then\line
        echo \u8220"SFILE e\u8217' um arquivo comum.\u8221"\line
      fi\line
    done\line
  else\line
    echo \u8220"uso: testa [arquivo... ]\u8221"\line
  fi\line
